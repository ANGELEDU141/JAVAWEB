public class barografo {


	@Component("Bean1")
	public class barografo implements Instrumento {

	    // metodo constructor
	    public Termometro() {

	    }

	    // Metodo heredado
	    @Override
	    public void medir() {
	        System.out.println(" Â¡Medicion por Termometro!");
	    }
	}


}
